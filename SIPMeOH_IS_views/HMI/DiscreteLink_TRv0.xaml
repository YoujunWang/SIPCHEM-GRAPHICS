<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:hmi="clr-namespace:SIPMeOH_IS_views.HMI"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity">

    <Style TargetType="{x:Type hmi:DiscreteLink_TRv0}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type hmi:DiscreteLink_TRv0}">
                    <Canvas
                        Width="24"
                        Height="16"
                        Background="Transparent">
                        <Ellipse
                            Name="DiscIndication1"
                            Canvas.Left="0.5"
                            Canvas.Top="1.5"
                            Width="21"
                            Height="15"
                            Fill="#FF000000"
                            Stroke="#FFFFFFFF"
                            StrokeDashArray=""
                            StrokeEndLineCap="Round"
                            StrokeLineJoin="Round"
                            StrokeMiterLimit="10"
                            StrokeStartLineCap="Round"
                            StrokeThickness="1" />
                    </Canvas>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>