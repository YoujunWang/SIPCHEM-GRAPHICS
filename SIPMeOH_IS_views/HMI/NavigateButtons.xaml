<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:hmi="clr-namespace:SIPMeOH_IS_views.HMI"
    xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity">

    <Style TargetType="{x:Type hmi:NavigateButtons}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type hmi:NavigateButtons}">
                    <Canvas
                        Width="97"
                        Height="33"
                        Background="Transparent">
                        <Image
                            Name="bmpNextPicture"
                            Canvas.Left="48"
                            Width="48"
                            Height="32"
                            Source="..\GraphicViews\Translations\Images\B_BT1201_bmpNextPicture_0.png" />
                        <Image
                            Name="bmpPreviousPicture"
                            Width="48"
                            Height="32"
                            Source="..\GraphicViews\Translations\Images\B_BT1201_bmpPreviousPicture_0.png" />
                    </Canvas>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
</ResourceDictionary>